{
  "name": "linkedin-react",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cypress": "^3.1.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "react-scripts": "^1.1.4",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "antd": "^3.8.4",
    "immer": "^1.5.0",
    "kitsu": "^6.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "cypress": "./node_modules/.bin/cypress open"
  },
  "husky": {
    "hooks": {
